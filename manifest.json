{
  "manifest_version": 3,
  "name": "SnapStash",
  "version": "1.0",
  "description": "Save shopping items from any website with categories",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "SnapStash - Shopping List"
  },
  "icons": {
    "128": "images/icon128.png"
  }
}
